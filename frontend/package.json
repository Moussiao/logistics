{
  "name": "logistic-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p check:type build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "check:type": "vue-tsc --build --force",
    "check:lint": "eslint . --max-warnings=0",
    "check:format": "prettier --check .",
    "fix:lint": "eslint . --fix --max-warnings=0",
    "fix:format": "prettier --write .",
    "check": "run-p check:*",
    "fix": "run-p fix:*",
    "add:component": "pnpx hygen new component --folder components --name",
    "add:view": "pnpx hygen new component --folder views --name",
    "add:layout": "pnpx hygen new component --folder layouts --name"
  },
  "dependencies": {
    "@telegram-apps/sdk": "1.1.3",
    "@vueuse/core": "^12.2.0",
    "axios": "^1.7.9",
    "camelcase-keys": "^9.1.3",
    "dayjs": "^1.11.13",
    "decamelize-keys": "^2.0.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-multiselect": "^3.1.0",
    "vue-router": "^4.5.0",
    "vue-tabler-icons": "^2.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "~9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.2.0"
  }
}
